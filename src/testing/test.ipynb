    "import cv2\n",
    "import pandas as pd\n",
    "\n",
    "dataSetPath = r'H:\\Adam\\School\\Coding\\Chroma.AI\\data\\color_names.csv'\n",
    "index=[\"Name\", \"Hex\", \"Red\", \"Green\", \"Blue\"]\n",
    "dataSet = pd.read_csv(dataSetPath, names=index, header=None)\n",
    "\n",
    "\n",
    "# Color Recognition\n",
    "r = g = b = xpos = ypos = 0\n",
    "def recognizeColor(R,G,B):\n",
    "    print('color recognized')\n",
    "    minimum = 10000\n",
    "    for i in range(len(dataSet)):\n",
    "        d = abs(R - int(dataSet.loc[i,\"Red\"])) \n",
    "        + abs(G - int(dataSet.loc[i,\"Green\"])) \n",
    "        + abs(B - int(dataSet.loc[i,\"Blue\"]))\n",
    "\n",
    "        if(d<=minimum):\n",
    "            minimum = d\n",
    "            cname = dataSet.loc[i,\"Name\"]\n",
    "    return cname\n",
    "\n",
    "\n",
    "def click_event(event, x, y, flags, param):\n",
    "    if event == cv2.EVENT_LBUTTONDOWN:\n",
    "        color = img[y, x]  # Get the color of the clicked pixel\n",
    "        color = color.tolist()  # Convert color to a Python list\n",
    "        color = [int(c) for c in color]  # Convert color values to integers\n",
    "        \n",
    "        r, g, b = color[0], color[1], color[2]\n",
    "\n",
    "        # Display a rectangle at the top of the window with the clicked color\n",
    "        cv2.rectangle(img, (0, 0), (imgWidth, 28), color, -1)\n",
    "\n",
    "        if r + g + b >= 300:\n",
    "            textColor = (0, 0, 0)\n",
    "        else:\n",
    "            textColor = (255, 255, 255)\n",
    "        \n",
    "        cv2.putText(img, f\"{recognizeColor(r, g, b)} | {r} , {g} , {b}\", (10, 20), \n",
    "                    cv2.FONT_HERSHEY_SIMPLEX, 0.7, textColor, 1)\n",
    "\n",
    "        # Show the modified image\n",
    "        cv2.imshow(\"Image\", img)\n",
    "\n",
    "# Load the image\n",
    "img = cv2.imread(r\"H:\\Adam\\School\\Coding\\Chroma.AI\\img\\testing\\test_img.jpg\")\n",
    "imgHeight, imgWidth, imgChannels = img.shape\n",
    "# Create a window and bind the mouse callback function\n",
    "cv2.namedWindow(\"Image\")\n",
    "cv2.setMouseCallback(\"Image\", click_event)\n",
    "\n",
    "# Show the original image\n",
    "cv2.imshow(\"Image\", img)\n",
    "\n",
    "# Wait for a key press to exit\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
